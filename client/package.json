{
  "name": "angify",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config ./proxy.conf.json --open",
    "build": "ng build --watch",
    "build:prod": "ng build",
    "build:prod:cordova": "ng build --prod --base-href . --output-path ./CordovaMobileApp/www/",
    "cordova:init": "cordova create CordovaMobileApp com.yantrab.app CordovaMobileApp",
    "cordova:build": "npm run cordova:init&&npm run build:prod:cordova",
    "cordova:run:browser": "cd CordovaMobileApp&&cordova platform add browser&&cordova run browser",
    "cordiva:add:android": "cordova platform add android",
    "cordova:run:android": "cd CordovaMobileApp&&cordova plugin add cordovarduino&&cordova run android",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "core-js": "^2.5.4",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26",
    "@angular/cdk": "^7.3.3",
    "@angular/flex-layout": "^7.0.0-beta.23",
    "@angular/material": "^7.3.3",
    "cordova-plugin-usbserial": "0.0.1",
    "hammerjs": "^2.0.8",
    "ngx-socket-io": "^2.1.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.5",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/node": "~8.9.4",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2",
    "angular-builder-custom-terser-options": "^1.0.1"
  }
}
